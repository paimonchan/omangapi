[options]
; copy and paste odoo.conf to root-project/odoo

; odoo configuration
db_host = localhost
db_port = 5432
db_user = <db_user>
db_password = <db_password>
db_name = <db_name>
addons_path = addons,../custom-addons
list_db = False

logfile=/var/log/odoo/odoo-omangapi.log

; manga source
source_mangadex = https://api.mangadex.org